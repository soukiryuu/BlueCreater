<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.bluecreater.ManagementDetailActivity"
    tools:showIn="@layout/activity_main">

    <TableLayout
        android:layout_width="395dp"
        android:layout_height="wrap_content"
        android:stretchColumns="0,1,2,3"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <include
            android:id="@+id/management_view_1"
            layout="@layout/management_detail_1" />

        <include
            android:id="@+id/management_view_2"
            layout="@layout/management_detail_2" />

        <include
            android:id="@+id/management_view_3"
            layout="@layout/management_detail_3" />

    </TableLayout>
</android.support.constraint.ConstraintLayout>
